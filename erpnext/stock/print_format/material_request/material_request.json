 "css": "@media all {\r\n   div .printedby {\r\n       position: fixed;\r\n       bottom: 0;\r\n       text-align: center;\r\n       font-size: xx-small;\r\n       width: 100% !important;\r\n   }\r\n}\r\ntable {\r\n    margin: 0 auto; /* or margin: 0 auto 0 auto */\r\n    width: 90%;\r\n    \r\n  }\r\n  \r\n\r\n\r\n#main {\r\n   margin-left: 20px;\r\n   margin-right: 20px;\r\n}\r\n\r\n\r\n    \r\n",
 "custom_format": 1,
 "default_print_language": "en",
 "disabled": 0,
 "doc_type": "Material Request",
 "docstatus": 0,
 "doctype": "Print Format",
 "font_size": 14,
 "html": "\r\n<div id=\"header-html\" class=\"hidden-pdf\">\r\n\t{%- if letter_head and not no_letterhead -%}\r\n\t\t<div class=\"letter-head\">\r\n\t\t\t{{ letter_head }}\r\n\t\t</div>\r\n\t{%- endif -%}\r\n</div>\r\n\r\n<div id=\"main\">\r\n    <div style=\"text-align: center;\">\r\n        <h3>MATERIAL REQUEST</h3>\r\n        <h5><u>\r\n        {{ doc.branch }}\r\n        </u></h5>\r\n    </div>\r\n    \r\n    <div style=\"margin-top: 10px;\">\r\n       <table class=\"table1\" style=\"margin-top: 1px; text-align:justify;font-size:small; width:100%;\">\r\n        <tr>\r\n            <td><strong>MR Date:</strong></td>\r\n            <td>{{ frappe.utils.formatdate(doc.transaction_date) }}</td>\r\n            <td><strong>Series:</strong></td>\r\n            <td>{{ doc.name }}</td>\r\n        </tr>\r\n        <tr>\r\n            <td><strong>Status:</strong></td>\r\n            <td>{{ doc.workflow_state }}</td>\r\n            <td><strong>Approved Date:</strong></td>\r\n            <td>{{ frappe.utils.formatdate(doc.approval_date) }}</td>\r\n        </tr>\r\n        <tr>\r\n            <td><strong>Title:</strong></td>\r\n            <td>{{ doc.title }}</td>\r\n            <td><strong>Purchase Change Date:</strong></td>\r\n            <td>{{ frappe.utils.formatdate(doc.date) }}</td>\r\n        </tr>\r\n       </table>\r\n        \r\n       <table class=\"table table-bordered\" style=\"margin-top: 5px;width:100%; font-size: small\">\r\n         <thead style=\"background-color: #f2f2f2;\">\r\n        <tr>\r\n            <th><strong>Sr<strong></th>\r\n            <th><strong>Code<strong></th>\r\n            <th><strong>Name<strong></th>\r\n            <th><strong>Group<strong></th>\r\n            <th><strong>UoM<strong></th>\r\n            <th><strong>Quantity<strong></th>\r\n            <th><strong>Estimated Cost<strong></th>\r\n            <th><strong>Delivery Date<strong></th>\r\n            <th><strong>Reason<strong>n</th>\r\n        </tr>\r\n        </thead>\r\n        \r\n        <tbody>\r\n        {%- set est_total = 0 -%}\r\n        {%- for row in doc.items -%}\r\n        <tr>\r\n            <td style=\"width: 3%;\">{{ row.idx }}</td>\r\n            <td style=\"width: 7%;\">{{ row.item_code }}</td>\r\n            <td style=\"width: 30%; text-align: left;\">{{ row.item_name }}</td>\r\n            <td style=\"width: 7%;\">{{ row.item_group }}</td>\r\n            <td style=\"width: 7%;\">{{ row.uom }}</td>\r\n            <td style=\"width: 7%;\">{{ row.qty }}</td>\r\n            <td style=\"width: 7%; text-align:right;\">{{ frappe.format_value(row.estimated_cost, {\"fieldtype\":\"Currency\"}) }}</td>\r\n            <td style=\"width: 16%; text-align: left;\">{{ frappe.utils.formatdate(row.schedule_date) }}</td>\r\n            <td style=\"width: 7%;\">{{ row.description }}</td>\r\n        </tr>\r\n        {%- endfor -%}\r\n    </tbody>\r\n</table>\r\n        \r\n       <div style=\"padding-top: 10px; width: 50%; float: right; text-align:right;\">\r\n         <strong>Total Estimate Cost: {{ frappe.format_value(doc.items|sum(attribute='estimated_cost'), {\"fieldtype\":\"Currency\"})}}</strong>\r\n       </div>\r\n\r\n       <div style=\"padding-top: 100px; clear: both;\">\r\n            <div>Authorized Signatory</div>\r\n        \r\n            <div style=\"padding-top: 80px; width: 50%; float: left; text-align:center;\">(Request By): {{ frappe.get_fullname(doc.owner) }} </div>\r\n            <div style=\"padding-top: 80px; width: 50%; float: left; text-align:center;\">(Approved By): {{ frappe.get_fullname(doc.approver) }} </div>\r\n       </div>\r\n    </div>\r\n</div>\r\n<div id=\"footer-html\" class=\"visible-pdf\">\r\n\t{% if not no_letterhead and footer %}\r\n\t\t<div class=\"letter-head-footer\">\r\n\t\t{{ footer }}\r\n\t\t</div>\r\n\t{% endif %}\r\n\r\n\t<p class=\"text-center small page-number visible-pdf\">\r\n\t\t{{ _(\"Page {0} of {1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }}\r\n\t</p>\r\n</div>\r\n",
