 "html": "<div class=\"page-break\">\r\n<div id=\"header-html\" class=\"hidden-pdf\">\r\n    {%- if letter_head and not no_letterhead -%}\r\n        <div class=\"letter-head\">\r\n            {{ letter_head }}\r\n        </div>\r\n    {%- endif -%}\r\n</div>\r\n<div id=\"footer-html\" class=\"visible-pdf\">\r\n    {% if not no_letterhead and footer %}\r\n        <div class=\"letter-head-footer\">\r\n        {{ footer }}\r\n        </div>\r\n    {% endif %}\r\n\r\n    <p class=\"text-center small page-number visible-pdf\">\r\n        {{ _(\"Page {0} of {1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }}\r\n    </p>\r\n</div>\r\n<div id=\"main\">\r\n\r\n\t<div class=\"print-heading\">\r\n\t\t<div  style=\"text-align: center;\">\r\n<h3>{{doc.naming_series}}</h3>\r\n<h5><u>\r\n{{doc.branch}}\r\n</u></h5>\r\n</div>\r\n\r\n\t\t\r\n<table class = \"table1\" style=\"margin-top: 1px; text-align:justify;font-size:small; width:100% \">\r\n<tr>\r\n<td><strong>Title:</strong></td>\r\n<td>Invoice</td>\r\n<td><strong>Posting Date:</strong></td>\r\n<td>{{ frappe.utils.formatdate(doc.posting_date) }}</td>\r\n</tr>\r\n<tr>\r\n<td><strong>Invoice Type:</strong></td>\r\n<td>{{doc.voucher_type}}</td>\r\n<td><strong>Reference:</strong></td>\r\n<td>{{doc.name}}</td>\r\n</tr>\r\n</table>\r\n\r\n\t</div>\r\n\r\n\t\r\n\t<table class=\"table table-bordered\" style=\"margin-top: 15px;\">\r\n\t\t<tbody>\r\n\t\t\t<tr>\r\n\t\t\t\t<th>Sr</th>\r\n\t\t\t\t<th>Account</th>\r\n\t\t\t\t<th>Party Type</th>\r\n\t\t\t\t<th>Party</th>\r\n\t\t\t\t<th class=\"text-right\">Debit</th>\r\n\t\t\t\t<th class=\"text-right\">Credit</th>\r\n\t\t\t</tr>\r\n\t\t\t{%- for row in doc.accounts -%}\r\n\t\t\t<tr>\r\n\t\t\t\t<td style=\"width: 3%;\">{{ row.idx }}</td>\r\n\t\t\t\t<td style=\"width: 37%;\">{{ row.account }}</td>\r\n\r\n\t\t\t\t<td style=\"width: 10%;\">\r\n\t\t\t\t\t{%- if row.party_type -%}\r\n\t\t\t\t\t\t{{ row.party_type }}\r\n\t\t\t\t\t{%- endif -%}\r\n\t\t\t\t</td>\r\n\r\n\t\t\t\t<td style=\"width: 20%;\">\r\n\t\t\t\t\t{%- if row.party -%}\r\n\t\t\t\t\t\t{{ row.party }}\r\n\t\t\t\t\t{%- endif -%}\r\n\t\t\t\t</td>\r\n\t\t\t\t<td style=\"width: 15%; text-align: right;\">{{ '{0:,.2f}'.format(row.debit) }}</td>\r\n\t\t\t\t<td style=\"width: 15%; text-align: right;\">{{ '{0:,.2f}'.format(row.credit) }}</td>\r\n\t\t\t</tr>\r\n\t\t\t{%- endfor -%}\r\n\t\t\t<tr>\r\n\t\t\t<td colspan=4; style=\"text-align: left\"><strong> Total </strong></td>\r\n\t\t\t<td style=\"text-align: right\">{{frappe.format_value(doc.total_debit, {\"fieldtype\":\"Currency\"})}}</td>\r\n\t\t\t<td style=\"text-align: right\">{{frappe.format_value(doc.total_credit, {\"fieldtype\":\"Currency\"})}}</td>\r\n\t\t</tr>\r\n\t\t</tbody>\r\n\t</table>\r\n\r\n\t<table>\r\n\t\t<tr>\r\n\t\t<td colspan=2; style=\"text-align: left\"><strong> Amount in words: </strong></td>\r\n\t\t<td colspan=4; style=\"text-align: left\">{{frappe.utils.money_in_words(doc.total_credit)}}</td>\r\n\t</tr>\r\n</table>\r\n\t\t<div style=\"padding-top: 130px; width: 40%; float: left; text-align:left;\">Invoice Raised By</div>\r\n\t\t<div style=\"padding-top: 130px; width: 40%; float: right; text-align:Right;\">Acknowledged By</div>\r\n\t\t<div style=\"padding-top: 250px; float: center; text-align:center;\"><b> Approved By</b></div>\r\n\t</div>\r\n<div style=\"padding-top: 100px; clear: both;\">\r\n<div class=\"printedby\">This print is system generated and should be considered VALID only with appropriate seal and/or signature. <br>  Printed by {{ frappe.user }} on {{ frappe.utils.nowdate()  }} at {{ frappe.utils.nowtime() }}</div>",
