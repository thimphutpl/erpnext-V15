 "html": "<div class=\"page-break\">\r\n\t<div id=\"header-html\" class=\"hidden-pdf\">\r\n    {%- if letter_head and not no_letterhead -%}\r\n        <div class=\"letter-head\">\r\n            {{ letter_head }}\r\n        </div>\r\n    {%- endif -%}\r\n</div>\r\n<div id=\"footer-html\" class=\"visible-pdf\">\r\n    {% if not no_letterhead and footer %}\r\n        <div class=\"letter-head-footer\">\r\n        {{ footer }}\r\n        </div>\r\n    {% endif %}\r\n\r\n    <p class=\"text-center small page-number visible-pdf\">\r\n        {{ _(\"Page {0} of {1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }}\r\n    </p>\r\n</div>\r\n<div class=\"print-heading\">\r\n\t\t<div  style=\"text-align: center;\">\r\n<h3>IMPREST REGISTER</h3>\r\n<h5><u>\r\n{{doc.branch}}\r\n</u></h5>\r\n</div>\r\n\r\n  <div >\r\n    <table class = \"table1\" style=\"margin-top: 10px; text-align:justify;font-size:small; width:100% \">\r\n\t\t\t\r\n\t<tr >\r\n\t <td width =\"140\" style = \"width:25%;\"> <strong>Title:</strong></td>\r\n\t<td>{{doc.title}}</td>\r\n\t\r\n\t<tr>\r\n\t\r\n\r\n\t<td><strong>Receipt Date: </strong></td>\r\n\t<td > {{frappe.utils.formatdate(doc.entry_date)}}</td>\r\n     <tr>\r\n\t<tr> <td> <strong> Cost Center: </strong> </td>\r\n\t<td> {{ doc.cost_center}}</td>\r\n </tr>\r\n\r\n\r\n\r\n\t    </table>\r\n  </div>\r\n\t\r\n\r\n <table class=\"table table-bordered\" style=\"margin-top: 15px; font-size: small\">\r\n  <thead>\r\n        <tr>\r\n          <th style=\"text-align: center;\" rowspan=\"4\" ;>Sr</th>\r\n          \r\n\t      <th style=\"text-align: center;\" colspan=\"7\" ;>Purchase</th>\r\n        </tr>\r\n        <tr>\r\n\t<th style=\"text-align: center;\">Date</th>\r\n    <th style=\"text-align: center;\">Particulars</th>\r\n    <th style=\"text-align: center;\">Budget Head</th>\r\n<th style=\"text-align: center;\">Remarks</th>\r\n          <th style=\"text-align: center;\">Qty</th>\r\n          <th style=\"text-align: center;\">Rate</th>\r\n          <th style=\"text-align: center;\">Amount</th>\r\n\t\r\n\r\n\t\t\t\t\t</tr> </thead>\r\n        \r\n<tr><tbody>\r\n\r\n          {%- for row in doc.items -%}\r\n\t\t\t\t\t\t\t<td style=\"width: 3%;\">  {{ row.idx }}</td>\r\n<td style=\"width: 13%;\">{{ row.invoice_date}}</td>\t\t\t\t\t\t\t\r\n                            <td style=\"width: 30%;\">{{ row.item_name}}</td>\r\n                            <td style=\"width: 30%\"> {{ row.account}} </td>\r\n\t\t\t\t\t\t\t<td>{{ row.remarks }}</td>\r\n\t\t\t\t\t\t\t<td style=\"width: 10%; text-align:center\">{{ row.quantity }}</td>\r\n\t\t\t\t\t\t\t<td style=\"width: 10%;text-align:center \">{{ row.rate }}</td>\r\n\t\t\t\t\t\t\t<td style=\"width: 15%; text-align: right\">{{ frappe.format_value (row.amount, {\"fieldtype\":\"Currency\"})}}</td>\r\n\t\t\r\n\t\t\t\t\t\t\t\t\t\t\t\t\r\n\t\t\t\t\t\t</tr>\r\n\t\r\n\r\n          {%- endfor -%}\r\n\r\n</tbody>\r\n</table>\r\n<table style=\"margin-top: 15px; font-size: small; width: 100%;\">\r\n<tr>\r\n<tr>\r\n<td style= \"text-align:right;\" > <strong>Openning Balance:</td>\r\n<td style= \"text-align:right;\"><strong>{{ frappe.format_value (doc.opening_balance, {\"fieldtype\":\"Currency\"})}}</td>\r\n</tr>\r\n<tr>\r\n<td style= \"text-align:right;\" ><strong>Purchase Amount:</td><td style= \"text-align:right;\"><strong>{{ frappe.format_value (doc.total_amount, {\"fieldtype\":\"Currency\"})}}</td>\r\n</tr>\r\n<tr>\r\n<td style= \"text-align:right;\" ><strong>Closing Balance:</td><td style= \"text-align:right;\"><strong>{{ frappe.format_value (doc.balance_amount, {\"fieldtype\":\"Currency\"})}}</td>\r\n</tr>\r\n</tr>\r\n</table>\r\n\r\n\r\n\r\n\t</small>\r\n</div>\r\n\r\n<div style=\"padding-top: 100px; clear: both;\">\r\n  <div class=\"printedby\">This print is system generated and should be considered VALID only with appropriate seal and/or signature. <br> Printed by {{ frappe.user }} on {{ frappe.utils.nowdate() }} at {{ frappe.utils.nowtime() }}\r\n  </div>",
